    "def get_correct_orientation(beta, node, speed, tol):\n",
    "    if abs(beta) < tol:\n",
    "        print(\"right angle\")\n",
    "        ctrl.stop_motors(node)\n",
    "        return(True)\n",
    "    if beta < 0 :\n",
    "        ctrl.set_motor_speed(speed, -speed, node)\n",
    "        return(False)\n",
    "    elif beta > 0:\n",
    "        ctrl.set_motor_speed(-speed, speed, node)\n",
    "        return(False)"
   ]
